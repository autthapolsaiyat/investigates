{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg,json,woff,woff2}"]
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data: blob:;",
    "X-Content-Type-Options": "nosniff"
  },
  "routes": [
    {
      "route": "/*.js",
      "headers": {
        "Content-Type": "text/javascript"
      }
    }
  ]
}
